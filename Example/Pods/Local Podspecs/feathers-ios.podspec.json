{
  "name": "feathers-ios",
  "version": "0.1.0",
  "summary": "FeathersJS iOS Client",
  "description": "Feathers-iOS is an iOS feathersJS client, writen in Swift 2.2. Connect, authenticate,\nand interact with any feathersjs api with ease.",
  "homepage": "https://github.com/feathersjs/feathers-ios",
  "license": "MIT",
  "authors": {
    "Brendan Conron": "conronb@gmail.com"
  },
  "source": {
    "git": "https://github.com/feathers/feathers-ios.git",
    "tag": "0.1.0"
  },
  "social_media_url": "https://twitter.com/feathersjs",
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "Pod/Classes/Core/**/*.{swift}"
      ],
      "platforms": {
        "ios": "8.0",
        "osx": "10.10",
        "watchos": "2.0",
        "tvos": "9.0"
      }
    }
  ]
}
